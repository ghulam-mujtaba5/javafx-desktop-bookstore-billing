<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" spacing="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.t.MenuController">
    <Button fx:id="addStockButton" text="Add Stock" onAction="#addStockButtonClicked"/>
    <Button fx:id="viewStockButton" text="View Stock" onAction="#viewStockButtonClicked"/>
    <Button fx:id="createOrderButton" text="Create Order" onAction="#createOrderButtonClicked"/>
    <Button fx:id="viewOrderButton" text="View Order" onAction="#viewOrderButtonClicked"/>
    <Button fx:id="viewInvoicesButton" text="View Invoices" onAction="#viewInvoicesButtonClicked"/>
    <Button fx:id="exitButton" text="Exit" onAction="#exitButtonClicked"/>
</VBox>
